{
	"name": "Knuth Closure Test",
	"file_name": "closure",
	"scale": {
		"type": "relative",
		"to": "base",
		"axis_scale": "@ZTD_IDK_BENCHMARKS_CLOSURES_GRAPH_SCALE@"
	},
	"categories": [
		{
			"name": "Man or Boy @ZTD_IDK_BENCHMARKS_CLOSURES_GRAPH_SCALE_TITLE@\n(@ZTD_IDK_BENCHMARKS_CLOSURES_GRAPH_PROVENANCE@)",
			"file_name": "@ZTD_IDK_BENCHMARKS_CLOSURES_GRAPH_SCALE@",
			"pattern": ".*",
			"ascending": true,
			"description": "A benchmark for determining the runtime of a Wikibooks / Rosetta Code example under many different kinds of languages."
		}
	],
	"data_groups": [
		{
			"name": "Custom C++ Class",
			"pattern": "custom_callable_cxx",
			"description": "The use of a custom-written C++ class with operator() in the Man or boy tests."
		},
		{
			"name": "C++03 shared_ptr (Rosetta Code)",
			"exact_pattern": "cxx03_shared_ptr_rosetta",
			"description": "The use of a std::shared_ptr with a run() function in the Man or boy tests. Does not use operator()."
		},
		{
			"name": "Lambdas",
			"exact_pattern": "lambda",
			"description": "The use of C++-style lambdas in the Man or boy tests. Uses templates to never erase the type: hard-codes recursion limit."
		},
		{
			"name": "Lambdas (No Function Helpers)",
			"exact_pattern": "lambda_all_lambdas",
			"description": "The use of C++-style lambdas in the Man or boy tests. Uses templates to never erase the type: hard-codes recursion limit, uses no regular helper functions."
		},
		{
			"name": "Lambdas (std::function)",
			"pattern": "lambda_std_function$",
			"description": "The use of C++-style lambdas in the Man or boy tests. Wraps the lambdas in a std::function."
		},
		{
			"name": "Lambdas (std::function_ref)",
			"pattern": "lambda_function_ref$",
			"description": "The use of C++-style lambdas in the Man or boy tests. Wraps the lambdas in a std::function_ref."
		},
		{
			"name": "Lambdas (Rosetta Code)",
			"exact_pattern": "lambda_rosetta",
			"description": "The use of C++-style lambdas in the Man or boy tests"
		},
		{
			"name": "Normal Functions",
			"exact_pattern": "plain_normal_functions",
			"description": "The use of plain C functions in the Man or Boy tests. Modifies the function arguments list to transfer data. Does not have an eval helper like in the Rosetta code functions."
		},
		{
			"name": "Normal Functions (Rosetta Code)",
			"pattern": "normal_functions_rosetta",
			"description": "The use of plain C functions in the Man or Boy tests. Modifies the function arguments list to transfer data."
		},
		{
			"name": "Normal Functions (Statics)",
			"pattern": "normal_function_statics",
			"description": "The use of plain C functions with statics for data transfer in the Man or Boy tests."
		},
		{
			"name": "GNU Nested Functions",
			"exact_pattern": "gnu_nested_functions",
			"description": "The use of the GNU Nested Functions C extension for the Man or Boy tests."
		},
		{
			"name": "GNU Nested Functions (Rosetta Code)",
			"pattern": "gnu_nested_functions_rosetta",
			"description": "The use of the GNU Nested Functions C extension for the Man or Boy tests."
		},
		{
			"name": "Apple Blocks",
			"pattern": "apple_blocks",
			"description": "The use of the Apple Blocks extension for the Man or Boy tests."
		}
	],
	"data_labels": [
		{
			"name": "real time",
			"id": "real_time",
			"format": "clock",
			"primary": true
		},
		{
			"name": "cpu time",
			"id": "cpu_time",
			"format": "clock"
		}
	],
	"remove_suffixes": [
		",",
		"_"
	],
	"remove_prefixes": [
		",",
		"_"
	]
}
